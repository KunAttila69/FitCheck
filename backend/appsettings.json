{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Data/fitcheck.db"
  },
  "JwtSettings": {
    "Key": "your-very-secure-secret-key-should-be-256-bit-long",
    "Issuer": "FitCheck-Server",
    "Audience": "FitCheck-User",
    "AccessTokenExpiryMinutes": 15,
    "RefreshTokenExpiryDays": 7
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
